{
  "name": "dush-methods",
  "version": "1.0.3",
  "description": "Plugin for `dush` and anything based on it. It adds helper `.define` and `.delegate` methods",
  "repository": "tunnckoCore/dush-methods",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard index.js test.js --fix --verbose",
    "test": "npm-run-all -s lint test:*",
    "test:api": "nyc --reporter lcov node test.js",
    "test:report": "nyc report",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "git": "npm-run-all -s git:*",
    "git:add": "git add --all",
    "git:cz": "git-cz",
    "commit": "npm-run-all -s test git"
  },
  "dependencies": {},
  "devDependencies": {
    "commitizen": "~2.7.0",
    "cz-conventional-changelog": "1.1.5",
    "dush": "^3.0.0",
    "mukla": "^0.4.9",
    "npm-run-all": "~3.1.2",
    "nyc": "^10.1.2",
    "pre-commit": "^1.2.2",
    "standard": "^9.0.0",
    "standard-version": "^4.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "baseplugin",
    "define",
    "delegate",
    "dush",
    "dushplugin",
    "enum",
    "enumerable",
    "helper",
    "methods",
    "micro",
    "minibaseplugin",
    "plugins"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "gruu-api",
        "dush",
        "dush-tap-report",
        "dush-no-chaining",
        "gibon",
        "dush-promise",
        "dush-router",
        "dush-options",
        "dush-methods"
      ],
      "highlight": "dush-tap-report"
    },
    "reflinks": [
      "always-done",
      "async-done",
      "base",
      "charlike",
      "commitizen",
      "dezalgo",
      "once",
      "standard-version",
      "verb",
      "verb-generate-readme",
      "dush",
      "minibase"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
